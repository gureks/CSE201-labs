import java.io.*;
import java.text.DecimalFormat;

/**
 * 
 * @author Gurek Singh 2015033
 * @author Saurabh Kapur 2015087
 *
 */

public class Main
{
	
	public static void readTemp()
	{
		float[][] temperature = new float[5][100];
		BufferedReader br = null;
		DecimalFormat df = new DecimalFormat("###.##");

		try 
		{
			System.out.println("reading file");
			
			br = new BufferedReader(new FileReader("temperature"));
			String next = null;

			for(int i=0;i<5;i++)
			{
				for(int j=0;j<100;j++)
				{	
					next = br.readLine();
					float temp = Float.valueOf(next);
					temperature[i][j] = Float.valueOf(df.format(temp));
				}
				String temp = br.readLine();
			}
		}	

		catch (FileNotFoundException ex)
		{
			System.out.println("File not found");
			ex.printStackTrace();
		}
		
		catch (IOException ex)
		{
			ex.printStackTrace();
		}
		
		for(int i=0;i<5;i++)
		{	
			for(int j=0;j<100;j++)
				System.out.println(temperature[i][j]);
			System.out.println("\n");
		}
	}
	public static void main(String[] args)
	{
		readTemp();
	}

}
