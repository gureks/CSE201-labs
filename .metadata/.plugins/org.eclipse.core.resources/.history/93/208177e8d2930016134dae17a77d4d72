import java.awt.event.*;
import java.util.*;
import javax.swing.*;

public class TicTacToe
{
	ArrayList<JButton> grid;
	
	public TicTacToe()
	{
		grid= new ArrayList<JButton>();
		
		for(int i=0; i<9; i++)
		{
			JButton temp = new JButton("");
			grid.add(temp);
			temp.addActionListener(new ActionListener(){
				public void actionPerformed(ActionEvent e)
				{
					
				}
			});
		}
	}
}
